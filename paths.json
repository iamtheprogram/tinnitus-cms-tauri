{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "*": ["./src/*"],
            "@src/*": ["./src/*"],
            "@components/*": ["./src/components/*"],
            "@utils/*": ["./src/utils/*"],
            "@store/*": ["./src/store/*"],
            "@pages/*": ["./src/pages/*"],
            "@services/*": ["./src/services/*"],
            "@config/*": ["./src/config/*"]
        }
    }
}
